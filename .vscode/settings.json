{
  "cSpell.words": [
    "academykit",
    "arkbotech",
    "ASPNETCORE",
    "Autosize",
    "Bijay",
    "caprover",
    "chartjs",
    "cref",
    "datatable",
    "DATETIME",
    "Dtos",
    "Esewa",
    "exif",
    "filepond",
    "fscreen",
    "HMACSHA",
    "immer",
    "incharge",
    "Includable",
    "inheritdoc",
    "ISMTP",
    "katex",
    "Lingtren",
    "Maharajgunj",
    "mantine",
    "minio",
    "nepaliname",
    "Newtonsoft",
    "officedocument",
    "openxmlformats",
    "paramref",
    "passcode",
    "prestart",
    "PRNG",
    "setuid",
    "spreadsheetml",
    "tabler",
    "tanstack",
    "threadsafety",
    "tinyint",
    "tiptap",
    "typeparam",
    "typeparamref",
    "v'VVV",
    "Vurilo",
    "Weightage"
  ],
  "dotnet.defaultSolution": "lingtren.sln",
  "search.exclude": {
    "**/src/Api/ClientApp/node_modules": true
  },
  "prettier.enable": true,
  "editor.trimAutoWhitespace": true,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "editor.comments.insertSpace": true,
  "editor.codeActionsOnSave": [
    "source.organizeImports",
    "source.addMissingImports"
  ],
  "typescript.autoClosingTags": true,
  "typescript.suggest.autoImports": true,
  "html.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "editor.minimap.maxColumn": 80,
  "csharp.format.enable": true,
  "editor.stickyScroll.enabled": true,
  "eslint.options": {
    "configFile": "../academykit.client/.eslintrc.cjs"
  }
}